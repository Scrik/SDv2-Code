{
    "name": "arrow768/sdv2",
    "description": "SourceDonatesv2",
    "require": {
        "laravel/framework": "4.1.*",
        "arrow768/sdv2pp-paypal": "@dev",
        "cartalyst/sentry": "3.0.*"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
